@{
    ViewData["Title"] = "Home Page";
}

@model BookListViewModel

@foreach (var x in Model.Books)
{

    <div>
        <h3>Title: @x.Title</h3>
        <p>Author: @x.Author</p>
        <p>Category: @x.Category</p>
        <p>Price: @x.Price</p>
        <p>Publisher: @x.Publisher</p>
        <p>ISBN: @x.ISBN</p>
    </div>
}

<div page-model="@Model.PagingInfo" page-action="Index">
 
</div>


